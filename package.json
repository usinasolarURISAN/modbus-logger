{
  "name": "modbus",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "tsc && node dist/index.js",
    "show-ports": "ls /dev/tty*",
    "dev": "ts-node-dev --transpile-only src/index.ts",
    "dev:arduino:0": "ENV=dev DEVICE=arduino DEVICE_ID=0 ts-node-dev --transpile-only src/index.ts",
    "dev:arduino:1": "ENV=dev DEVICE=arduino DEVICE_ID=1 ts-node-dev --transpile-only src/index.ts",
    "dev:epever:0": "ENV=dev DEVICE=epever DEVICE_ID=0 ts-node-dev --transpile-only src/index.ts",
    "dev:epever:1": "ENV=dev DEVICE=epever DEVICE_ID=1 ts-node-dev --transpile-only src/index.ts",
    "dev:goodwe:0": "ENV=dev DEVICE=goodwe DEVICE_ID=0 ts-node-dev --transpile-only src/index.ts",
    "prd:epever:0": "ENV=prd DEVICE=epever DEVICE_ID=0 ts-node-dev --transpile-only src/index.ts",
    "prd:epever:1": "ENV=prd DEVICE=epever DEVICE_ID=1 ts-node-dev --transpile-only src/index.ts",
    "prd:goodwe:0": "ENV=prd DEVICE=goodwe DEVICE_ID=0 ts-node-dev --transpile-only src/index.ts",
    "test:arduino": "ENV=dev DEVICE=arduino ts-node-dev --transpile-only src/read.test.ts",
    "test:epever": "ENV=dev DEVICE=epever DEVICE_ID=0 ts-node-dev --transpile-only src/read.test.ts",
    "test:read": "ts-node-dev --transpile-only src/read.test.ts",
    "test": "echo \"Error: no test specified\" && exit 1",
    "typeorm": "ts-node-dev node_modules/typeorm/cli.js",
    "dev:arduino:0:migrations:run": "ENV=dev DEVICE=arduino DEVICE_ID=0 yarn typeorm migration:run",
    "dev:arduino:1:migrations:run": "ENV=dev DEVICE=arduino DEVICE_ID=1 yarn typeorm migration:run",
    "dev:arduino:0:migrations:create": "ENV=dev DEVICE=arduino DEVICE_ID=0 yarn typeorm migration:create -n CreateArduino",
    "dev:arduino:1:migrations:create": "ENV=dev DEVICE=arduino DEVICE_ID=1 yarn typeorm migration:create -n CreateArduino",
    "dev:arduino:0:migrations:revert": "ENV=dev DEVICE=arduino DEVICE_ID=0 yarn typeorm migration:revert",
    "dev:arduino:1:migrations:revert": "ENV=dev DEVICE=arduino DEVICE_ID=1 yarn typeorm migration:revert",
    "dev:epever:migrations:run": "ENV=dev DEVICE=epever yarn typeorm migration:run",
    "dev:epever:migrations:create": "ENV=dev DEVICE=epever yarn typeorm migration:create -n CreateEpever",
    "dev:epever:migrations:revert": "ENV=dev DEVICE=epever yarn typeorm migration:revert",
    "dev:goodwe:migrations:run": "ENV=dev DEVICE=goodwe yarn typeorm migration:run",
    "dev:goodwe:migrations:revert": "ENV=dev DEVICE=goodwe yarn typeorm migration:revert",
    "prd:epever:migrations:run": "ENV=prd DEVICE=epever yarn typeorm migration:run",
    "prd:epever:migrations:create": "ENV=prd DEVICE=epever yarn typeorm migration:create -n CreateEpever",
    "prd:epever:migrations:revert": "ENV=prd DEVICE=epever yarn typeorm migration:revert",
    "prd:goodwe:migrations:run": "ENV=prd DEVICE=goodwe yarn typeorm migration:run",
    "prd:goodwe:migrations:revert": "ENV=prd DEVICE=goodwe yarn typeorm migration:revert",
    "clean": "rm -fr node_modules && rm -fr build && yarn",
    "prettier:format": "prettier --config .prettierrc 'src/**/*.{ts,tsx}' --write"
  },
  "devDependencies": {
    "@types/cron": "^1.7.3",
    "@types/node": "^16.11.1",
    "@types/uuid": "^8.3.1",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^16.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "^2.5.0",
    "ts-node-dev": "^1.1.8",
    "tslint": "5.12.1",
    "typescript": "3.3.3"
  },
  "dependencies": {
    "cron": "^1.8.2",
    "firebase": "^9.8.4",
    "modbus-serial": "8.0.5",
    "pg": "^8.7.1",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.2.38",
    "uuid": "^8.3.2"
  }
}
